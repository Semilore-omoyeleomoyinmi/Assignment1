<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableCell?>
<?import javafx.scene.control.TextField?>


<GridPane fx:controller="sample.Controller" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

    <TableView fx:id="table" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <columns>
            <TableColumn fx:id="file" text="FILE"></TableColumn>
            <TableColumn fx:id="actualClass" text="Actual Class"></TableColumn>
            <TableColumn fx:id="spamProb" text="Spam Probability"></TableColumn>
        </columns>
    </TableView>

    <Label text="Accuracy:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField id="accuracy" fx:id="accuracy" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <Label text="Precision:" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <TextField id="precision" fx:id="precision" GridPane.columnIndex="0" GridPane.rowIndex="4"/>

</GridPane>